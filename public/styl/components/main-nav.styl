.main-nav
  background-color: yellow // this should be sand
  height: 40px
  width: 90%
  margin: 50px auto

  .title
    float: left
    cursor: pointer
    font-size: 24px
    padding: 5px
    margin-left: 30px

  ul
    float: right

    li
      display: inline-block
      padding: 10px

      a
        color: black
        text-decoration: none

        &:hover
          text-decoration: underline